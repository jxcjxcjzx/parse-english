{
  "name": "parse-english",
  "version": "2.0.0",
  "description": "English (natural language) parser",
  "license": "MIT",
  "keywords": [
    "nlcst",
    "english",
    "natural",
    "language",
    "parser"
  ],
  "dependencies": {
    "array-iterate": "^1.0.0",
    "nlcst-to-string": "^1.0.0",
    "parse-latin": "^2.0.0",
    "unist-util-modify-children": "^1.0.0",
    "unist-util-visit-children": "^1.0.0"
  },
  "repository": "wooorm/parse-english",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^12.0.0",
    "chalk": "^1.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^2.0.0",
    "remark-lint": "^2.0.0",
    "remark-slug": "^3.0.0",
    "remark-validate-links": "^2.0.0",
    "mocha": "^2.0.0",
    "nlcst-test": "^0.2.0",
    "regenerate": "^1.0.0",
    "unicode-8.0.0": "^0.1.0"
  },
  "scripts": {
    "test-api": "mocha --check-leaks test/index.js",
    "test-coverage": "istanbul cover _mocha -- test/index.js",
    "test-travis": "npm run test-coverage",
    "test": "npm run test-api",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "make": "npm run lint && npm run test-coverage",
    "fixture": "node script/generate-fixture",
    "bundle": "browserify index.js -s ParseEnglish > parse-english.js",
    "postbundle": "esmangle parse-english.js > parse-english.min.js",
    "build-md": "remark . --quiet",
    "build": "npm run build-md && npm run bundle"
  }
}
